@model IEnumerable<Semana13_2_AJAX_Cine.Models.Interprete>

<p>
    @Html.ActionLink("Nuevo", "Crear")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Paterno)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Materno)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Nombres)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nacionalidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Foto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Paterno)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Materno)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Nombres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nacionalidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Foto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bio)
        </td>
        <td>
            @Html.ActionLink("Editar", "Editar", new { id=item.CodInterprete }) |
            @Html.ActionLink("Detalles", "Detalles", new { id=item.CodInterprete }) |
            @Html.ActionLink("Eliminar", "Eliminar", new { id=item.CodInterprete })
        </td>
    </tr>
}

</table>
